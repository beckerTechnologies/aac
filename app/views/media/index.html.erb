<h1>Listing media</h1>

<table>
  <thead>
    <tr>
      <th>Response</th>
      <th>Label</th>
      <th>Description</th>
      <th>Data</th>
      <th>Filename</th>
      <th>Type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @media.each do |medium| %>
      <tr>
        <td><%= medium.response_id %></td>
        <td><%= medium.label %></td>
        <td><%= medium.description %></td>
        <td><%= medium.data %></td>
        <td><%= medium.filename %></td>
        <td><%= medium.type %></td>
        <td><%= link_to 'Show', medium %></td>
        <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
        <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medium', new_medium_path %>
